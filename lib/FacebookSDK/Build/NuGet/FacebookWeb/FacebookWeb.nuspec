<?xml version='1.0'?>
<package xmlns='http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd'>
    <metadata>
        <id>FacebookWeb</id>
        <version><%= version_full %></version>
        <title/>
        <authors>Jim Zimmerman, Nathan Totten, Prabir Shrestha</authors>
        <description>The Facebook C# SDK web component.</description>
        <language>en-US</language>
        <licenseUrl>http://facebooksdk.codeplex.com/license</licenseUrl>
        <projectUrl>http://facebooksdk.codeplex.com</projectUrl>
        <iconUrl>http://bit.ly/facebooksdkicon</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <tags>Facebook</tags>
        <dependencies>
            <dependency id='Facebook' version='<%= version_full %>'/>
        </dependencies>
    </metadata>
	<files>
		<file src="..\..\Build\NuGet\FacebookWeb\content\**\*.*" target = "content" />
		<file src="..\..\Build\NuGet\FacebookWeb\tools\**\*" target = "tools" />
		<file src="..\..\Bin\Release\net40\Facebook.Web.dll" target = "lib\net40" />
		<file src="..\..\Bin\Release\net40\Facebook.Web.xml" target = "lib\net40" />
		<file src="..\..\Bin\Release\net40\CodeContracts\Facebook.Web.Contracts.dll" target = "lib\net40\CodeContracts" />
		<file src="..\..\Bin\Release\net35\Facebook.Web.dll" target = "lib\net35" />
		<file src="..\..\Bin\Release\net35\Facebook.Web.xml" target = "lib\net35" />
		<file src="..\..\Bin\Release\net35\CodeContracts\Facebook.Web.Contracts.dll" target = "lib\net35\CodeContracts" />
	</files>
</package>
